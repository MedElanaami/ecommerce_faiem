                                                    <img src="{{ asset('uploads/images/produits/'~produit.imageName) }}"
                                                         alt="product" width="80" height="80">
                                                </a>

                                                <a href="{{ path('app_supprimer_produit', {'key': key}) }}" class="btn-remove" title="Remove Product"><span>×</span></a>
